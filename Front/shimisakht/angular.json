{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shimisakht": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/shimisakht",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/Theme/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/Theme/vendor/fontawesome-free/css/all.min.css",
              "src/assets/Theme/vendor/animate/animate.compat.css",
              "src/assets/Theme/vendor/simple-line-icons/css/simple-line-icons.min.css",
              "src/assets/Theme/vendor/owl.carousel/assets/owl.carousel.min.css",
              "src/assets/Theme/vendor/owl.carousel/assets/owl.theme.default.min.css",
              "src/assets/Theme/vendor/magnific-popup/magnific-popup.min.css",
              "src/assets/Theme/css/rtl-theme.css",
              "src/assets/Theme/css/rtl-theme-elements.css",
              "src/assets/Theme/css/rtl-theme-blog.css",
              "src/assets/Theme/css/rtl-theme-shop.css",
              "src/assets/Theme/css/demos/rtl-demo-industry-factory.css",
              "src/assets/Theme/css/skins/skin-industry-factory.css",

              "src/styles.scss"
            ],
            "scripts": [
              "src/assets/Theme/vendor/modernizr/modernizr.min.js",
              "src/assets/Theme/vendor/jquery/jquery.min.js",
              "src/assets/Theme/vendor/jquery.appear/jquery.appear.min.js",
              "src/assets/Theme/vendor/jquery.easing/jquery.easing.min.js",
              "src/assets/Theme/vendor/jquery.cookie/jquery.cookie.min.js",
              "src/assets/Theme/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/Theme/vendor/jquery.validation/jquery.validate.min.js",
              "src/assets/Theme/vendor/jquery.easy-pie-chart/jquery.easypiechart.min.js",
              "src/assets/Theme/vendor/jquery.gmap/jquery.gmap.min.js",
              "src/assets/Theme/vendor/lazysizes/lazysizes.min.js",
              "src/assets/Theme/vendor/isotope/jquery.isotope.min.js",
              "src/assets/Theme/vendor/owl.carousel/owl.carousel.min.js",
              "src/assets/Theme/vendor/magnific-popup/jquery.magnific-popup.min.js",
              "src/assets/Theme/vendor/vide/jquery.vide.min.js",
              "src/assets/Theme/vendor/vivus/vivus.min.js",
              "src/assets/Theme/js/theme.js",
              "src/assets/Theme/js/views/view.contact.js",
              "src/assets/Theme/js/demos/demo-industry-factory.js",
              "src/assets/Theme/js/custom.js",
              "src/assets/Theme/js/theme.init.js"

            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "shimisakht:build:production"
            },
            "development": {
              "browserTarget": "shimisakht:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shimisakht:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
